  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "deploy": "nx deploy",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "clean": "./scripts/clean.sh",
    "dev-services": "nx dev-services",
    "dev": "nx dev",
    "dev-init": "nx dev-init",
    "analyze": "cross-env ANALYZE=true nx build",
    "e2e": "nx e2e",
    "e2e-ci": "node ./scripts/e2e-ci.js",
    "system-e2e": "playwright test -c apps/system-e2e/src",
    "codegen": "nx codegen",
    "translations": "nx translations",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:libs": "nx affected:libs",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:docker": "nx affected --target=docker",
    "affected": "nx affected",
    "security-audit": "bash -c 'yarn audit --level high ; [[ \"$?\" -lt  \"8\" ]]'",
    "license-audit": "license-checker --failOn 'GPL; AGPL; UNLICENSED' --summary",
    "generate": "nx generate",
    "get-secrets": "./scripts/get-secrets.sh",
    "create-secret": "./scripts/create-secret.sh",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ts-node": "cross-env ts-node -r tsconfig-paths/register -O '{\\\"module\\\":\\\"commonjs\\\",\\\"esModuleInterop\\\":true}'",
    "ts": "node -r ./scripts/ts-node.js",
    "openapi-generator": "openapi-generator generate -g typescript-fetch --additional-properties=typescriptThreePlus=true",
    "schemas": "node ./scripts/schemas.js",
    "postinstall": "node -e 'process.exit(process.env.CI || process.env.SKIP_POSTINSTALL ? 0 : 1);' || node ./scripts/postinstall.js",
    "gitbook": "node ./scripts/gitbook.js",
    "check-tags": "yarn ts scripts/check-tags.ts",
    "charts": "cd ./infra && yarn charts",
    "infra": "cd ./infra && yarn cli"
  },
